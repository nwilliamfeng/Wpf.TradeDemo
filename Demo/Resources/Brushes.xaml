<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Demo.Resources">
    <!--白色-->
    <Color x:Key="white-color-1">#FFF4E1C0</Color>
    <Color x:Key="white-color-2">#FFFFEBC8</Color>
    <!--黑色-->
    <Color x:Key="black-color">#FF000000</Color>
    <!--橙色1-->
    <Color x:Key="orange-color-1">#FFFF9300</Color>
    <!--墨绿色1-->
    <Color x:Key="blackish-green-color-1">#FF136C5E</Color>
    <!--墨绿色2-->
    <Color x:Key="blackish-green-color-2">#FF193D37</Color>
    <!--墨绿色3-->
    <Color x:Key="blackish-green-color-3">#FF172422</Color>

    <Color x:Key="red-color-1">#E81123</Color>
    

    <SolidColorBrush x:Key="window-title-brush" Color="{StaticResource blackish-green-color-2}"/>

    <SolidColorBrush x:Key="window-border-brush" Color="{StaticResource blackish-green-color-2}"/>
    
    <SolidColorBrush x:Key="button-confirm-brush" Color="{StaticResource blackish-green-color-1}"/>

    <SolidColorBrush x:Key="window-title-foreground" Color="{StaticResource white-color-1}"/>

    <SolidColorBrush x:Key="control-foreground" Color="{StaticResource white-color-2}"/>

    <SolidColorBrush x:Key="window-close-brush" Color="{StaticResource red-color-1}"/>

</ResourceDictionary>